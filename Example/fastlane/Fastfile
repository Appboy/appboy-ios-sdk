# Change the syntax highlighting to Ruby
# All lines starting with a # are ignored when running `fastlane`

# If you want to automatically update fastlane if a new version is available:
# update_fastlane

# This is the minimum version number required.
# Update this, if you use features of a newer version
fastlane_version "1.99.0"

lane :matchFullAccess do
  match(type: "development", app_identifier: "com.appboy.stopwatch", force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch", force_for_new_devices: true)
  match(type: "appstore", app_identifier: "com.appboy.stopwatch", force_for_new_devices: true)
  match(type: "development", app_identifier: "com.appboy.stopwatch.StopwatchNotificationService", force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch.StopwatchNotificationService", force_for_new_devices: true)
  match(type: "development", app_identifier: "com.appboy.stopwatch.StopwatchNotificationContentExtension", force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch.StopwatchNotificationContentExtension", force_for_new_devices: true)
end

lane :matchReadOnly do
  match(type: "development", app_identifier: "com.appboy.stopwatch", readonly: true, force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch", readonly: true, force_for_new_devices: true)
  match(type: "appstore", app_identifier: "com.appboy.stopwatch", readonly: true, force_for_new_devices: true)
  match(type: "development", app_identifier: "com.appboy.stopwatch.StopwatchNotificationService", readonly: true, force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch.StopwatchNotificationService", readonly: true, force_for_new_devices: true)
  match(type: "development", app_identifier: "com.appboy.stopwatch.StopwatchNotificationContentExtension", readonly: true, force_for_new_devices: true)
  match(type: "adhoc", app_identifier: "com.appboy.stopwatch.StopwatchNotificationContentExtension", readonly: true, force_for_new_devices: true)
end

